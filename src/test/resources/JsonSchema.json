{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Schema for Single User Object",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "yearsOld": {
      "type": "number"
    },
    "courses": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "id",
    "name",
    "location",
    "phone",
    "yearsOld",
    "courses"
  ]
}
